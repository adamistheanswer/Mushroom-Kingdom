    "nipplejs": "^0.9.1",
    "nodemon": "^2.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.143.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
